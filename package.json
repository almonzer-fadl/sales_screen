{
  "name": "daqaiq-v2",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build:admin": "cd apps/admin && npm run build",
    "build:supplier": "cd apps/supplier && npm run build",
    "build:store": "cd apps/store && npm run build",
    "build": "turbo run build",
    "dev:admin": "cd apps/admin && npm run dev",
    "dev:supplier": "cd apps/supplier && npm run dev",
    "dev:store": "cd apps/store && npm run dev",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{js,jsx,md}\"",
    "clean": "turbo run clean && rm -rf node_modules",
    "start": "turbo run start",
    "analyze": "ANALYZE=true turbo run build"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "daisyui": "^5.0.35",
    "lucia": "^3.2.2",
    "mongodb": "^6.16.0",
    "mongoose": "^8.14.1",
    "next": "^15.3.1",
    "nodemailer": "^6.9.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sharp": "^0.33.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.3.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "postcss": "^8.5.3",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.4.1",
    "turbo": "latest"
  },
  "packageManager": "npm@10.2.4",
  "engines": {
    "node": ">=18.0.0"
  }
}
